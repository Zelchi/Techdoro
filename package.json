{
    "name": "techdoro",
    "productName": "techdoro",
    "version": "2.0.0",
    "description": "Techdoro is a pomodoro timer application built with Electron and React.",
    "main": ".vite/build/main.js",
    "scripts": {
        "dev": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "make:win": "electron-forge make --platform=win32",
        "make:linux": "electron-forge make --platform=linux",
        "make:darwin": "electron-forge make --platform=darwin",
        "make:all": "npm run make:win && npm run make:linux && npm run make:darwin",
        "publish": "electron-forge publish",
        "postinstall": "patch-package"
    },
    "keywords": [],
    "author": {
        "name": "Zelchi",
        "email": "tininhakawaii@gmail.com"
    },
    "license": "MIT",
    "devDependencies": {
        "@electron-forge/cli": "^7.8.3",
        "@electron-forge/maker-deb": "^7.10.2",
        "@electron-forge/maker-rpm": "^7.10.2",
        "@electron-forge/maker-squirrel": "^7.10.2",
        "@electron-forge/maker-zip": "^7.10.2",
        "@electron-forge/plugin-vite": "^7.10.2",
        "@types/electron-squirrel-startup": "^1.0.2",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.2",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "electron": "^39.1.0",
        "eslint": "^8.57.1",
        "eslint-plugin-import": "^2.32.0",
        "patch-package": "^8.0.1",
        "typescript": "^4.5.4",
        "vite": "^7.2.1"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^2.10.1",
        "electron-installer-redhat": "^3.4.0",
        "electron-squirrel-startup": "^1.0.1",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-icons": "^5.5.0",
        "react-redux": "^9.2.0",
        "styled-components": "^6.1.19"
    }
}