{
  "name": "techdoro",
  "private": true,
  "version": "0.1.2",
  "main": "dist/electron/main.js",
  "author": {
    "name": "Zelchi",
    "email": "tininhakawaii@gmail.com"
  },
  "description": "Techdoro Ã© um Pomodoro retro!",
  "homepage": "./",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "electron:dev": "cross-env NODE_ENV=DEV electron public/electron/main.js",
    "electron:win": "npm run build && electron-builder --w",
    "electron:mac": "npm run build && electron-builder --m",
    "electron:linux": "npm run build && electron-builder --l"
  },
  "build": {
    "appId": "com.techdoro",
    "productName": "Techdoro",
    "win": {
      "icon": "dist/iconTechDoro.png"
    },
    "files": [
      "dist/**/*"
    ],
    "directories": {
      "buildResources": "public"
    }
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "react-redux": "^9.2.0",
    "styled-components": "^6.1.15"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "cross-env": "^7.0.3",
    "electron": "^34.1.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0",
    "vite-plugin-singlefile": "^2.1.0"
  }
}